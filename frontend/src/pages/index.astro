---
// import Star from "../components/Star";
import fetchApi from '../lib/strapi';
import type Article from '../interfaces/article';
import MainLayout from '../layouts/mainLayout.astro';
import Cursus from '../components/Cursus';
import Menu from '../components/Menu.astro';
import Timeline from '../components/TImeline';
import ProjectLayout from '../layouts/projectLayout';
import ChiFuMi from '../components/ChiFuMi';

const articles = await fetchApi<Article[]>({
  endpoint: 'articles', // the content type to fetch
  wrappedByKey: 'data', // the key to unwrap the response
});
---

<MainLayout>
  <Menu />
  <Cursus client:visible />
  <ProjectLayout client:load />
  <Timeline client:visible />
  <ChiFuMi client:visible />
</MainLayout>
